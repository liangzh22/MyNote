<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.mynote">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Home"></activity>
        <activity android:name=".register"></activity>
        <activity android:name=".AddNoteActivity"></activity>
        <activity android:name=".NoteActivity"></activity>
        <activity android:name=".ShowNoteActivity"></activity>
        <activity android:name=".DailyActivity"></activity>
        <activity android:name=".WriteDaily"></activity>
        <activity android:name=".ShowDailyActivity"></activity>
        <activity android:name=".ManageActivity"></activity>
        <activity android:name=".PasswordChange"></activity>
        <activity android:name=".PersonDetail"></activity>
        <activity android:name=".HabitActivity"></activity>
        <activity android:name=".ForgetPass"></activity>
        <activity android:name=".SetNewPass"></activity>
    </application>

</manifest>